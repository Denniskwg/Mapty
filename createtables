CREATE DATABASE mapty IF NOT EXISTS;
CREATE TABLE users IF NOT EXISTS (
    id VARCHAR(60) PRIMARY KEY,
    password VARCHAR(60),
    first_name VARCHAR(60),
    last_name VARCHAR(60),
    user_name VARCHAR(60),
    email VARCHAR(60)
);
CREATE TABLE workouts (
    id VARCHAR(60) PRIMARY KEY,
    password VARCHAR(60),
    first_name VARCHAR(60),
    last_name VARCHAR(60),
    user_name VARCHAR(60),
    email VARCHAR(60)
);

